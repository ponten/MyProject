create index G_QC_TEST_TYPE_TIME on G_QC_LOT_TEST_TYPE (START_TIME)
  tablespace SYSRTIDX
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 256K
    next 1M
