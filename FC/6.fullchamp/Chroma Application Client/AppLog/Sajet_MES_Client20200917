<log4j:event logger="ClientUtils" timestamp="1600303120499" level="ERROR" thread="1"><log4j:message>ExecuteSQL#2: SELECT
    AAA.PROCESS_CODE
   ,AAA.PROCESS_NAME
   ,AAA.WORK_ORDER
   ,AAA.RC_NO
   ,AAA.PART_NO
   ,AAA.SPEC1
   ,AAA.SPEC2
   ,BBB.MACHINE_CODE
   ,BBB.MACHINE_DESC
   ,BBB.TYPE_CODE
   ,BBB.TYPE_DESC
   ,BBB.REASON_CODE
   ,BBB.REASON_DESC
   ,BBB.REMARK
   ,BBB.START_TIME
   ,BBB.END_TIME
   ,BBB.WORK_TIME
FROM
    (
        SELECT
            SPS.PROCESS_CODE
           ,SPS.PROCESS_NAME
           ,GRT.TRAVEL_ID
           ,GRT.WORK_ORDER
           ,GRT.RC_NO
           ,SPT.PART_NO
           ,SPT.SPEC1
           ,SPT.SPEC2
        FROM
            (
                SELECT * FROM SAJET.G_RC_STATUS
                UNION
                SELECT * FROM SAJET.G_RC_TRAVEL
            ) GRT
           ,SAJET.SYS_PROCESS SPS
           ,SAJET.SYS_PART SPT
        WHERE 1 = 1
        AND GRT.PROCESS_ID = SPS.PROCESS_ID
        AND GRT.PART_ID = SPT.PART_ID
    ) AAA
   ,(
        SELECT
            GRTMD.TRAVEL_ID
           ,GRTMD.RC_NO
           ,SMDT.TYPE_CODE
           ,SMDT.TYPE_DESC
           ,SMDD.REASON_CODE
           ,SMDD.REASON_DESC
           ,SM.MACHINE_CODE
           ,SM.MACHINE_DESC
           ,TO_CHAR(GRTMD.START_TIME,'YYYY/MM/DD HH24:MI:SS') "START_TIME"
           ,TO_CHAR(GRTMD.END_TIME,'YYYY/MM/DD HH24:MI:SS') "END_TIME"
           ,GRTMD.WORK_TIME
           ,GRTMD.REMARK
        FROM
            SAJET.G_RC_TRAVEL_MACHINE_DOWN GRTMD
           ,SAJET.SYS_MACHINE_DOWN_DETAIL SMDD
           ,SAJET.SYS_MACHINE_DOWN_TYPE SMDT
           ,SAJET.SYS_MACHINE SM
        WHERE 1 = 1
        AND GRTMD.REASON_ID = SMDD.REASON_ID(+)
        AND SMDD.TYPE_ID = SMDT.TYPE_ID(+)
        AND GRTMD.MACHINE_ID = SM.MACHINE_ID
    ) BBB
WHERE 1 = 1
AND AAA.TRAVEL_ID = BBB.TRAVEL_ID(+)
AND AAA.RC_NO = BBB.RC_NO(+)






ORDER BY
    AAA.PROCESS_CODE
   ,AAA.RC_NO
   ,BBB.MACHINE_CODE
   ,BBB.START_TIME:ORA-00904: "GRTMD"."WORK_TIME": 無效的 ID</log4j:message><log4j:properties><log4j:data name="log4net:HostName" value="Robert-Dev-PC" /><log4j:data name="log4jmachinename" value="Robert-Dev-PC" /><log4j:data name="LogFileName" value="Sajet_MES_Client" /><log4j:data name="log4net:Identity" value="" /><log4j:data name="log4net:UserName" value="ROBERT-DEV-PC\Administrator" /><log4j:data name="log4japp" value="Sajet MES.exe" /></log4j:properties><log4j:locationInfo class="ClientUtils" method="addConnLog" file="" line="0" /></log4j:event>
