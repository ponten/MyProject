<log4j:event logger="ClientUtils" timestamp="1632898896731" level="ERROR" thread="1"><log4j:message>ExecuteSQL#2: 
WITH ROUTE_DETAIL AS (
    SELECT
        ROWNUM IDX,
        NODE_CONTENT,
        NODE_ID
    FROM
        SAJET.SYS_RC_ROUTE_DETAIL
    START WITH ROUTE_ID = :ROUTE_ID
               AND NODE_CONTENT = 'START' CONNECT BY PRIOR NEXT_NODE_ID = NODE_ID
                                                     OR PRIOR NEXT_NODE_ID = GROUP_ID
), PROCESSES AS (
    SELECT
        B.IDX,
        C.PROCESS_NAME,
        B.NODE_ID
    FROM
        ROUTE_DETAIL        B,
        SAJET.SYS_PROCESS   C
    WHERE
        B.NODE_CONTENT = TO_CHAR(C.PROCESS_ID)
), PROCESS_OPTIONS AS (
    SELECT
        PART_ID,
        ROUTE_ID,
        NODE_ID,
        PROCESS_ID,
        OPTION1
    FROM
        SAJET.SYS_PROCESS_OPTION_PART
    WHERE
        PART_ID = :PART_ID
        AND ROUTE_ID = :ROUTE_ID
)
SELECT
    A.PROCESS_NAME,
    A.NODE_ID,
    NVL(B.OPTION1, 0) STD_WORKTIME
FROM
    PROCESSES         A,
    PROCESS_OPTIONS   B
WHERE
    A.NODE_ID = B.NODE_ID (+)
ORDER BY
    IDX
