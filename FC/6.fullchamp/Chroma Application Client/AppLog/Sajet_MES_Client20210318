SELECT
    A.PROCESS_ID,
    A.PROCESS_NAME,
    A.PROCESS_DESC,
    A.PROCESS_DESC2,
    A.PROCESS_CODE,
    B.TYPE_NAME,
    A.WIP_ERP,
    NVL(C.OPTION1, 'N') OPTION1 PrintWaterProofLabel,
    NVL(C.OPTION2, 'N') OPTION2 UseT4Stove,
    NVL(C.OPTION3, 'N') OPTION3 UseT6Stove,
    NVL(C.OPTION4, 'N') OPTION4 StartingProcessOf10C,
    NVL(C.OPTION5, 'N') OPTION5 WarehousingProcessOf10C,
    TRIM(D.MACHINE_TYPE_NAME)
    || ' '
    || TRIM(D.MACHINE_TYPE_DESC) UsingEquipment,
    E.DEPT_NAME ProductionUnit
FROM
    SAJET.SYS_PROCESS          A,
    SAJET.SYS_OPERATE_TYPE     B,
    SAJET.SYS_PROCESS_OPTION   C,
    SAJET.SYS_MACHINE_TYPE     D,
    SAJET.SYS_DEPT             E
WHERE
