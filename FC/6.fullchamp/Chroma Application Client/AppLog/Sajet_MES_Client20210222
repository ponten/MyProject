WITH ROUTE_NODES AS (
    SELECT
        ROWNUM IDX,
        ROUTE_ID,
        NODE_CONTENT,
        NODE_ID
    FROM
        SAJET.SYS_RC_ROUTE_DETAIL
    START WITH ROUTE_ID = V_ROUTE_ID
               AND NODE_CONTENT = 'START' CONNECT BY PRIOR NEXT_NODE_ID = NODE_ID
                                                     OR PRIOR NEXT_NODE_ID = GROUP_ID
)
SELECT
    NVL(TRIM(B.ROUTE_NAME), '0') ROUTE_NAME,
