/* 指定流程卡的基本資訊 */
WITH RC_INFO AS (
    SELECT
        WORK_ORDER,
        RC_NO,
        PART_ID
    FROM
        SAJET.G_RC_STATUS
    WHERE
        RC_NO = :RC_NO
),
/* 所有屬於同工單的流程卡 */
ALL_RC AS (
    SELECT
        RC_NO
    FROM
        SAJET.G_RC_STATUS   A,
        RC_INFO             B
    WHERE
        A.WORK_ORDER = B.WORK_ORDER
    UNION
    SELECT
        A.RC_NO
    FROM
        SAJET.G_RC_TRAVEL   A,
        RC_INFO             B
    WHERE
        A.WORK_ORDER = B.WORK_ORDER
),
/* 該工單在指定製程蒐集到的資料 ID （一組資料共用一個 ID）*/
SN_IDS AS (
    SELECT DISTINCT
        SN_ID
    FROM
        SAJET.G_QC_VALUE   A,
        ALL_RC             B
    WHERE
        A.PROCESS_ID = :PROCESS_ID
        AND A.RC_NO = B.RC_NO
)
/* 共有幾筆 */
SELECT
    COUNT(SN_ID) SAMPLED_QTY
FROM
    SN_IDS
 】

