<log4j:event logger="ClientUtils" timestamp="1634708508668" level="ERROR" thread="1"><log4j:message>ExecuteSQL#2: SELECT X.MACHINE_CODE, X.TYPE_DESC, X.REASON_DESC, X.DOWN_TIME, ROUND(X.DOWN_TIME/Y.TOTAL_TIME, 4) * 100 PERCENT 
FROM 
(
SELECT 
A.MACHINE_ID, C.MACHINE_CODE, B.TYPE_ID, D.TYPE_DESC, B.REASON_DESC, SUM(A.WORK_TIME_MINUTE) DOWN_TIME, 1 SEQ 
FROM SAJET.G_MACHINE_DOWN_EXCLUDE A
JOIN SAJET.SYS_MACHINE_DOWN_DETAIL B ON A.REASON_ID=B.REASON_ID 
JOIN SAJET.SYS_MACHINE C ON A.MACHINE_ID=C.MACHINE_ID 
JOIN SAJET.SYS_MACHINE_DOWN_TYPE D ON B.TYPE_ID=D.TYPE_ID 
GROUP BY A.MACHINE_ID, C.MACHINE_CODE, B.TYPE_ID, D.TYPE_DESC, B.REASON_DESC 
UNION ALL 
SELECT A.MACHINE_ID, C.MACHINE_CODE, B.TYPE_ID, D.TYPE_DESC, D.TYPE_DESC, SUM(WORK_TIME_MINUTE), 2 
FROM SAJET.G_MACHINE_DOWN_EXCLUDE A
JOIN SAJET.SYS_MACHINE_DOWN_DETAIL B ON A.REASON_ID=B.REASON_ID 
JOIN SAJET.SYS_MACHINE C ON A.MACHINE_ID=C.MACHINE_ID 
JOIN SAJET.SYS_MACHINE_DOWN_TYPE D ON B.TYPE_ID=D.TYPE_ID 
GROUP BY A.MACHINE_ID, C.MACHINE_CODE, B.TYPE_ID, D.TYPE_DESC
) X 
JOIN 
(
     SELECT A.MACHINE_ID, B.TYPE_ID, SUM(A.WORK_TIME_MINUTE) TOTAL_TIME 
     FROM G_MACHINE_DOWN_EXCLUDE A
     JOIN SAJET.SYS_MACHINE_DOWN_DETAIL B ON A.REASON_ID=B.REASON_ID 
     GROUP BY A.MACHINE_ID, B.TYPE_ID
) Y ON X.MACHINE_ID=Y.MACHINE_ID AND X.TYPE_ID=Y.TYPE_ID 
WHERE 1=1
[ＡＮＤ　X.MACHINE_CODE=＠MACHINE_CODE]
〔ＡＮＤ　X.TYPE_DESC=＠TYPE_DESC〕
