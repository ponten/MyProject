    PARAM_PART_ID         SAJET.SYS_PART.PART%TYPE;
    PARAM_PROCESS_ID      SAJET.SYS_PROCESS.PROCESS_ID%TYPE;
    PARAM_NODE_ID         SAJET.SYS_RC_ROUTE_DETAIL.NODE_ID%TYPE;
